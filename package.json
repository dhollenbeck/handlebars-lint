{
  "name": "handlebars-lint",
  "description": "Lint for missing variables, helpers, block helpers or partials.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/handlebars-lint",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/handlebars-lint",
  "bugs": {
    "url": "https://github.com/jonschlinkert/handlebars-lint/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-union": "^3.1.0",
    "handlebars": "^4.0.11"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "find",
    "handlebars",
    "helpers",
    "lint",
    "missing",
    "template",
    "templates",
    "variables"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "handlebars-helpers",
        "template-helpers"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
